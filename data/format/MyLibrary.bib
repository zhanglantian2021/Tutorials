  lccn = {10.315},
  keywords = {sensitivity},
  annotation = {00000},
  file = {D\:\\zotero_data\\paper\\IbÃ¡Ã±ez et al_2007_EXPLOITING TEMPORAL VARIABILITY TO UNDERSTAND.pdf}
}

