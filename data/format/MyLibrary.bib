@article{cunyGeneralizedAdditiveModels2013,
  title = {Generalized Additive Models Reveal the Intrinsic Complexity of Wood Formation Dynamics},
  shorttitle = {å¹¿ä¹‰å¯åŠ æ¨¡å‹æ­ç¤ºäº†æœ¨æå½¢æˆåŠ¨åŠ›å­¦çš„å†…åœ¨å¤æ‚æ€§},
