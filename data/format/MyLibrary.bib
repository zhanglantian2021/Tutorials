@article{rossiApplicationGompertzEquation2003,
  title = {Application of the {{Gompertz}} Equation for the Study of Xylem Cell Development},
  shorttitle = {Gompertz æ–¹ç¨‹åœ¨æœ¨è´¨éƒ¨ç»†èƒå‘è‚²ç ”ç©¶ä¸­çš„åº” ç”¨},
