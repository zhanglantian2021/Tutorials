@article{depickereEffectSocialEnvironmental2008,
  title = {Effect of Social and Environmental Factors on Ant Aggregation: {{A}} General Response?},
  shorttitle = {ç¤¾ä¼šå’Œç¯å¢ƒå› ç´ å¯¹èš‚èšèšé›†çš„å½±å“: ä¸€ä¸ªæ™®éçš„ååº” ï¼Ÿ},
