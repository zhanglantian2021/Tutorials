  lccn = {3.687},
  keywords = {ant_diet},
  annotation = {00000},
  file = {D\:\\zotero_data\\paper\\PÃ©rez-RodrÃ­guez et al_2021_Sugar provisioning for ants enhances biological.pdf}
}

