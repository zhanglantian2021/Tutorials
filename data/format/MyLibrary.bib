@article{abeEffectWaterStatus1999,
  title = {Effect of the Water Status within a Tree on Tracheid Morphogenesis in {{Cryptomeria}} Japonica {{D}}. {{Don}}},
  shorttitle = {æ ‘ä½“å†…æ°´åˆ†çŠ¶å†µå¯¹æ—¥æœ¬æŸ³æ‰ç®¡èƒå½¢æ€å»ºæˆçš„å½±å“},
